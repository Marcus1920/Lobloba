<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App11.Views.Sellers.SellersMaster"
             xmlns:pages="clr-namespace:App11.Views.Sellers"
                  xmlns:sellers="clr-namespace:App11.Views.Sellers;assembly=App11"
                 >
    <MasterDetailPage.Master>
        <sellers:SellersMasterMaster x:Name="MasterPage" />
    </MasterDetailPage.Master>

    <MasterDetailPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <sellers:SellersTabbedPage />
            </x:Arguments>
        </NavigationPage>
    </MasterDetailPage.Detail>
</MasterDetailPage>